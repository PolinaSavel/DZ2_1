//Задача 1
import java.util.Scanner;
public class Lesson_2_1 {
   public static void main(String[] args) {
         
  Scanner in = new Scanner(System.in);
  System.out.println("Input a number_1: ");
  int a = in.nextInt();
  System.out.println("Input a number_2: ");
  int b = in.nextInt();
  int c = b * a * (a + b) / (a * a);
  System.out.printf("Your number: %d \n", a);
  System.out.printf("Your number: %d \n", b);
  System.out.println("c= "+ c);
  in.close();

 }
}
